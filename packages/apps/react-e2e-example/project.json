{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/apps/react-e2e-example/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "packages/apps/react-e2e-example/cypress.json",
        "devServerTarget": "apps-react-example:serve:development"
      },
      "configurations": {
        "production": {
          "devServerTarget": "apps-react-example:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/apps/react-e2e-example/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["apps-react-example"]
}
